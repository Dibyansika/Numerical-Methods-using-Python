        "Newton method"
      ],
      "metadata": {
        "id": "yqehQS__8Ryu"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def f(x):\n",
        "    return x**2 -2\n",
        "def df(x):\n",
        "    return 2*x\n",
        "x0 = 2\n",
        "abs_err = 1\n",
        "count=0\n",
        "while abs(abs_err) > 0.0000001:\n",
        "    x1 = x0 - (f(x0)/df(x0))\n",
        "    abs_err= x1 - x0\n",
        "    x0 = x1\n",
        "    count+=1\n",
        "print(\"Therefore, value of √2 is approximately\", x0) "
      ],
